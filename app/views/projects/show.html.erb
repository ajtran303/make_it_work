<%= @project.name %>
<%= "Material: #{@project.material}" %>
<%= "Challenge Theme: #{@project.challenge.theme}" %>
<%= "Number of Contestants: #{@project.contestants.size}" %>
<%= "Average Contestant Experience: #{@project.average_contestant_experience} years" %>

<%= form_tag "/projects/#{@project.id}", method: :post do %>
  <%= label_tag :add_contestant_id %>
  <%= text_field_tag :add_contestant_id %>
  <%= submit_tag "Add Contestant To Project" %>
<% end %>
